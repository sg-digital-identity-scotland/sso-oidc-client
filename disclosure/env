SSL_ENABLED=true
SSL_KEY_STORE_NAME=oidc-private-client
SSL_KEY_STORE_PASSWORD=oidc-private-client
SSL_KEY_ALIAS=oidc-private-client
LOG_LEVEL=INFO
HOST=${HOST}
PORT=${PORT}
CLIENT_DOMAIN=https://${HOST}:${PORT}
REDIRECT_URI=${CLIENT_DOMAIN}/login/oauth2/code/dis
POST_LOGOUT_REDIRECT_URI=${CLIENT_DOMAIN}/logout/callback
BACKCHANNEL_LOGOUT_URI=${CLIENT_DOMAIN}/logout/back-channel
DIS_ISSUER_URI=https://authz.swan.sign-in.service.gov.scot
DIS_END_SESSION_URI=https://authz.swan.sign-in.service.gov.scot/authorize/logout
